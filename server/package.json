{
  "name": "walter",
  "version": "0.0.0",
  "private": true,
  "main": "lib/index.js",
  "scripts": {
    "start": "nodemon lib/index.js --exec babel-node",
    "build": "rm -fR dist && babel lib -d dist",
    "worker": "nodemon lib/worker/index.js --exec babel-node",
    "serve": "node --max-old-space-size=3096 dist/index.js",
    "add-tenant:dev": "babel-node lib/batches/index.js addTenant",
    "init-tenant-w:dev": "babel-node lib/batches/index.js initTenantW",
    "init-db:dev": "babel-node lib/batches/index.js initDb",
    "init-elasticsearch:dev": "babel-node lib/batches/index.js initElasticsearch",
    "re-create-elastic-cache:dev": "babel-node lib/batches/index.js reCreateElasticCache",
    "add-timestamp-setting:dev": "babel-node lib/batches/index.js addTimestampSetting",
    "add-tenant:prod": "node dist/batches/index.js addTenant",
    "init-tenant-w:prod": "node dist/batches/index.js initTenantW",
    "init-elasticsearch:prod": "node dist/batches/index.js initElasticsearch",
    "re-create-elastic-cache:prod": "node dist/batches/index.js reCreateElasticCache",
    "add-timestamp-setting:prod": "node dist/batches/index.js addTimestampSetting",
    "analyze": "babel-node batches/index.js analyze",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill --timeout 15000 lib/**/**/files.spec.js",
    "debug-re-create-elastic-cache": "babel-node batches/index.js reCreateElasticCache --inspect-brk=9229",
    "debug-add-timestamp-setting": "babel-node lib/batches/index.js addTimestampSetting --inspect-brk=9229",
    "move-unvisible-files": "babel-node batches/index.js moveUnvisibleFiles",
    "create-admin": "babel-node batches/index.js createAdmin",
    "delete-admin": "babel-node batches/index.js deleteAdmin",
    "debug_main": "nodemon lib/index.js --exec babel-node --inspect-brk=9229",
    "debug_worker": "nodemon lib/worker/index.js --exec babel-node --inspect-brk=9229",
    "debug_test": "mocha --compilers js:babel-core/register --require babel-polyfill --timeout 15000 lib/**/kafkaclient/*.spec.js --inspect-brk=9229"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.1.0",
    "axios": "^0.18.0",
    "babel-runtime": "6.26.0",
    "body-parser": "1.17.2",
    "co": "4.6.0",
    "cookie-parser": "1.4.3",
    "debug": "2.6.9",
    "elasticsearch": "16.1.1",
    "exceljs": "0.7.1",
    "express": "4.15.5",
    "file-type": "^10.10.0",
    "jade": "1.11.0",
    "jsonwebtoken": "7.4.3",
    "kafka-node": "4.1.1",
    "lodash": "4.17.4",
    "log4js": "4.0.2",
    "moment": "2.20.1",
    "mongoose": "^5.6.0",
    "morgan": "1.8.2",
    "multer": "1.3.0",
    "pkgcloud": "1.5.0",
    "q": "1.5.1",
    "serve-favicon": "2.4.5",
    "superagent-defaults": "0.1.14",
    "supertest": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "chai": "4.1.2",
    "espower-babel": "4.0.3",
    "flow-bin": "0.54.1",
    "forever": "0.15.3",
    "mocha": "3.5.3",
    "mongodb-memory-server-global": "^5.1.5",
    "morgan": "1.8.2",
    "nodemon": "1.18.7",
    "power-assert": "1.4.4",
    "sinon": "7.3.2"
  }
}
